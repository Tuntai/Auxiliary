{
    "manifest_version" : 2,
    "version": "1.0.0",
    "name":"Auxiliary",
    "description": "An all in one utility extension!",
    "icons":{
        "128":"icons/icon128.png",
        "48" :"icons/icon48.png",
        "16":"icons/icon16.png"
    },
    "browser_action":{
        "default_popup":"popup.html",
        "default_icon":"icons/icon16.png"
    },
    "background":{
        "scripts":["js/background.js"],
        "persistent": false
    },
    "permissions": ["tabs","activeTab","declarativeContent", "storage", "alarms", "notifications"] 
}